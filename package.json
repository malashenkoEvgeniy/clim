    "babel-loader-exclude-node-modules-except": "^1.0.1",
    "browser-sync": "^2.26.3",
    "browser-sync-dev-hot-webpack-plugin": "^0.2.2",
    "browser-sync-webpack-plugin": "^2.2.2",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "del": "^3.0.0",
    "eslint": "^5.15.0",
