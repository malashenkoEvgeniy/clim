{
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npm run site-build && npm run admin-build",
    "re-build": "npm run site-re-build && npm run admin-re-build",
    "test": "npm run site-test && npm run admin-test && npm run webpack-eslint",
    "***********************************************": "echo site",
    "site-build": "npm run site-test && npm run site-production -- --build",
    "site-re-build": "npm run site-test && npm run site-production -- --build --clear",
    "site-test": "npm run site-sass-lint && npm run site-eslint",
    "site-watch": "npm run site-development -- --watch --open",
    "site-watch-hot": "npm run site-development -- --watch --open --hot",
    "site-watch-production": "npm run site-production -- --watch --open",
    "site-watch-production-hot": "npm run site-production -- --watch --open --hot",
    "site-sass-lint": "sass-lint -c \"./.sass-lint-site.yml\" -v -q",
    "site-eslint": "eslint ./resources/assets/site/js/**/*.js ./resources/views/site/**/*.js ./app/Modules/**/Views/site/**/*.js --fix --cache --cache-location ./.cache/.eslintcache-site",
    "site-development": "cross-env NODE_ENV=development webpack --config=webpack.export.js --scan --app=site",
    "site-production": "cross-env NODE_ENV=production webpack --config=webpack.export.js --production --progress --scan --app=site",
    "------------------------------------------------": "echo admin",
    "admin-build": "npm run admin-test && npm run admin-production -- --build",
    "admin-re-build": "npm run admin-test && npm run admin-production -- --build --clear",
    "admin-test": "npm run admin-sass-lint && npm run admin-eslint",
    "admin-watch": "npm run admin-development -- --watch --open",
    "admin-watch-hot": "npm run admin-development -- --watch --open --hot",
    "admin-watch-production": "npm run admin-production -- --watch --open",
    "admin-watch-production-hot": "npm run admin-production -- --watch --open --hot",
    "admin-sass-lint": "sass-lint -c \"./.sass-lint-admin.yml\" -v -q",
    "admin-eslint": "eslint ./resources/assets/admin/js/**/*.js --fix --cache --cache-location ./.cache/.eslintcache-admin",
    "admin-development": "cross-env NODE_ENV=development webpack --config=webpack.export.js --app=admin",
    "admin-production": "cross-env NODE_ENV=production webpack --config=webpack.export.js --production --progress --app=admin",
    "================================================": "echo webpack",
    "webpack-eslint": "eslint ./webpack.config.js ./resources/webpack/**/*.js --fix --cache --cache-location ./.cache/.eslintcache-webpack"
  },
  "dependencies": {
    "browserizr": "^3.2.1",
    "clipboard": "^2.0.4",
    "create-timer": "^1.0.3",
    "custom-jquery-methods": "^1.6.0",
    "es6-promise-polyfill": "^1.2.0",
    "inputmask": "^4.0.6",
    "intersection-observer": "^0.5.0",
