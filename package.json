    "jquery-validation": "^1.19.0",
    "jquery.mmenu": "^7.3.2",
    "lodash.throttle": "^4.1.1",
    "lozad": "^1.9.0",
    "magnific-popup": "^1.1.0",
    "mdn-polyfills": "^5.16.0",
    "modernizr": "^3.7.0",
    "module-dynamic-import": "^0.4.0",
    "normalize.css": "^8.0.1",
    "noty": "^3.2.0-beta",
    "perfect-scrollbar": "^1.4.0",
    "sass-lint": "^1.12.1",
    "select2": "^4.0.6-rc.1",
    "slick-carousel": "^1.8.1",
    "tippy.js": "^4.0.4",
    "wezom-log": "^1.0.0",
    "wezom-module-loader": "^2.0.4",
    "wezom-standard-tabs": "^2.1.0",
    "zz-load": "^0.11.0"
